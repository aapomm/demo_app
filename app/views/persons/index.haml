= link_to 'Logout', :logout, :method => :delete

= form_tag '/persons/search', :method => :get do

  = text_field_tag 'query', nil, {:class => 'js-autocomplete', :data => {'autocomplete-limit' => '10', 'autocomplete' => '/persons/autocomplete'}}
  = submit_tag 'search'

= render :partial => 'table', :locals => {:persons => @persons}
